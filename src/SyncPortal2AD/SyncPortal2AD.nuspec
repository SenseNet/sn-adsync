<?xml version="1.0" encoding="utf-8"?>
<package xmlns="http://schemas.microsoft.com/packaging/2012/06/nuspec.xsd">
  <metadata>
    <id>SenseNet.SyncPortal2AD</id>
    <version>7.0.0</version>
    <title>sensenet ECM SyncPortal2AD</title>
    <authors>kavics,aniko,lajos,tusmester</authors>
    <owners>Sense/Net</owners>
    <licenseUrl>https://github.com/SenseNet/sn-adsync/blob/master/LICENSE</licenseUrl>
    <projectUrl>https://github.com/SenseNet/sn-adsync</projectUrl>
    <iconUrl>https://raw.githubusercontent.com/SenseNet/sn-resources/master/images/sn-icon/sensenet-icon-64.png</iconUrl>
    <requireLicenseAcceptance>false</requireLicenseAcceptance>
    <description>Portal to Active Directory synchronization tool for the sensenet ECM platform.</description>
    <releaseNotes>See release notes on GitHub.</releaseNotes>
    <copyright>Copyright Â© Sense/Net Inc.</copyright>
    <tags>sensenet ecm ecms ad sync</tags>
    <dependencies>
      <dependency id="SenseNet.Tools" version="2.1.1" />
    </dependencies>
  </metadata>
  <files>
    <file src="..\nuget\readme-syncportal2ad.txt" target="readme.txt" />
    <file src="..\nuget\content-syncportal2ad\**" target="content" />
    <file src="bin\Release\SenseNet.DirectoryServices.*" target="lib\net451" />
    
    <!-- Copy the tool 'manually'. If we compiled it directly to the target folder, it would contain all dlls unnecessarily. -->
    <file src="bin\Release\SyncPortal2AD.exe" target="content\Tools" />
    <file src="bin\Release\SyncPortal2AD.pdb" target="content\Tools" />
    <file src="bin\Release\SyncPortal2AD.exe.config" target="content\Tools" />
    <file src="bin\Release\SenseNet.DirectoryServices.*" target="content\Tools" />
  </files>
</package>